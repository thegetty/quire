{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "excludePattern": "(^|\\/|\\\\)_",
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
