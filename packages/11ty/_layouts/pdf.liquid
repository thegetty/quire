<!doctype html>
<html>
  <head>
    <!--
    <meta charset="utf-8" />
    <meta name="description" content="${description}">
    <meta name="keywords" content="${keywords}">
    <title>${pageTitle}</title>
    <link rel="canonical" href="${canonicalURL}">
    <link rel="version-history" href="${publication.repositoryUrl}">
