{
  "name": "11ty-quire-plugin",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "build": "ELEVENTY_ENV=production eleventy",
    "build:pdf": "npx pagedjs-cli@latest ${ELEVENTY_OUTPUT_DIR:-_site}/pdf.html",
    "clean": "del ${ELEVENTY_OUTPUT_DIR:-_site} public/pdf.html",
    "debug": "DEBUG=Eleventy* eleventy --dryrun",
    "dev": "ELEVENTY_ENV=development eleventy --serve",
    "prebuild": "npm run clean",
    "prepare": "cp -n .env.example .env",
    "predev": "npm run clean",
    "preserve": "npm run clean",
    "serve": "ELEVENTY_ENV=production eleventy --serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "eleventy",
    "quire"
  ],
