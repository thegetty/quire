    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "@11ty/eleventy-plugin-vite": "^1.0.0-canary.2",
    "@iiif/parser": "^1.0.10",
    "@iiif/vault": "^0.9.18",
    "chalk": "^4.1.2",
    "common-tags": "^2.0.0-alpha.1",
    "del-cli": "^4.0.1",
