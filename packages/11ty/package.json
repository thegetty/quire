  "version": "1.0.0",
  "description": "",
  "main": ".eleventy.js",
  "scripts": {
    "build": "ELEVENTY_ENV=production eleventy && vite build",
    "clean": "del ./_site",
    "debug": "DEBUG=Eleventy* npx eleventy --dryrun",
    "dev": "ELEVENTY_ENV=development $(eleventy --watch --serve) & vite serve _site",
    "prebuild": "npm run clean",
    "predev": "npm run clean",
    "prestart": "npm run build",
    "serve": "ELEVENTY_ENV=production eleventy && vite preview _site",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
